.navigator {
    width: 100%;
    height: 75rpx;
    box-sizing: border-box;
    padding-left: 20rpx;
    line-height: 75rpx;
    background: #5a8ff0;
    color: #fff;
    font-size: 32rpx;
    border-radius: 20rpx;
    box-shadow: 0 2px 20rpx rgba(0, 0, 0, 0.3);
    text-align: center;
    margin-bottom: 47rpx;
    letter-spacing: 20rpx
}

.navigator_fixed {
    width: 200rpx;
    height: 62rpx;
    box-shadow: 0 2px 20rpx rgba(0, 0, 0, 0.3);
    background: #5a8ff0;
    font-size: 26rpx;
    color: #fff;
    text-align: center;
    border-radius: 20rpx;
    line-height: 62rpx;
}